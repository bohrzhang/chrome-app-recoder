{
  "name": "超链组装器",
  "version": "1.0",
  "manifest_version": 2,
  "description": "超链接path和params记录器，让访问更简单!",
  "permissions": ["storage", "declarativeContent", "activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": "images/nokia_suite_mirror.png"
  },
  "options_page": "options.html"
}