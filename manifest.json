{
  "name": "loader",
  "version": "1.0",
  "manifest_version": 2,
  "description": "超链奥特曼",
  "permissions": ["storage", "declarativeContent", "activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/nokia_suite_mirror.png"
  },
  "options_page": "options.html"
}